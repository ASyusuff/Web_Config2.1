;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	yusuf.id. root.yusuf.id. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	yusuf.id.
3	IN	PTR	yusuf.id.
3	IN	PTR	www.yusuf.id.
3	IN	PTR	sub.yusuf.id.
